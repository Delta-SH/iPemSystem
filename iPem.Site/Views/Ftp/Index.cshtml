@{
    Layout = null;
    var title = Request.QueryString["title"] ?? "FTP文件管理";
}

<!DOCTYPE html>
<html>
<head>
    <title>@title</title>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="shortcut icon" type="image/x-icon" href="~/favicon.ico" />
    <link rel="icon" type="image/x-icon" href="~/favicon.ico" />
    <link rel="bookmark" type="image/x-icon" href="~/favicon.ico" />
    <link href="@Url.Content("~/Scripts/ext-4.2.1/theme/css/ext-all-neptune.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/ux/all.css?v=" + iPem.Site.Infrastructure.EngineContext.Current.AppStore.JSVersion)" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/css/ftp.css?v=" + iPem.Site.Infrastructure.EngineContext.Current.AppStore.JSVersion)" rel="stylesheet" type="text/css" />
</head>
<body>
    <script type="text/javascript">
        if (typeof window.$$iPems === 'undefined' || typeof window.$$iPems !== 'object')
            window.$$iPems = {};

        var $$FTP_TITLE = '@title';
        var $$FTP_KEY = '@(ViewBag.FTPKey != null ? ViewBag.FTPKey : "")';
    </script>
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="@Url.Content("~/Scripts/ext-4.2.1/ext-all.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/all.js?v=" + iPem.Site.Infrastructure.EngineContext.Current.AppStore.JSVersion)" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ext-4.2.1/locale/ext-lang-cn.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ftp.js?v=" + iPem.Site.Infrastructure.EngineContext.Current.AppStore.JSVersion)" type="text/javascript"></script>
</body>
</html>
